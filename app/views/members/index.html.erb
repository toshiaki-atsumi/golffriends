<h1>Members#index</h1>
 <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>氏名</th>
        <th>email</th>
      </tr>
    </thead>
    <tbody>
     <% @members.each do |member| %>
        <tr>
          <td><%= link_to member.id, member_path(member.id) %></td>
          <td><%= member.name %></td>
          <td><%=member.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>