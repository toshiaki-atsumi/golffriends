<h1>Members#index</h1>
 <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>会の名前</th>
        <th>氏名</th>
        <th>email</th>
      </tr>
    </thead>
    <tbody>
    <% @parties.each do |party| %>
      <% Request.where(party_id: party.id).each do |request| %>
       <% if request.status == '承認' %>
        <tr>
          <td><%= link_to request.member_id, member_path(request.member_id) %></td>
          <td><%= Party.find(request.party_id).name %></td>
          <td><%= Member.find(request.member_id).name %></td>
          <td><%= Member.find(request.member_id).email %></td>
        </tr>
        <% else %>
        <% end %>
      <% end %>
      <% end %>
    <% end %>
    </tbody>
</table>