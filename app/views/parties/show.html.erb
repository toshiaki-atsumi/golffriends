<h1>Parties#show</h1>

<table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>会の名前</th>
        <th>管理者</th>
        <th>ゴルフ場</th>
      </tr>
    </thead>
    <tbody>
     <% @parties.each do |party| %>
        <tr>
          <td><%= link_to party.id, edit_party_path(party) %></td>
          <td><%= party.name %></td>
          <td><%= Member.find(party.member_id).name %></td>
          <td><%= party.golfcourse_id %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
