<h1>Parties#show</h1>

<table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>会の名前</th>
        <th>申請状況</th>
        <th>申請日時</th>
        <th>更新日時</th>
      </tr>
    </thead>
    <tbody>
     <% @requests.each do |request| %>
        <tr>
          <td><%= request.id %></td>
          <td><%= Party.find(request.party_id).name %></td>
          <td><%= request.status %></td>
          <td><%= request.created_at.to_date %></td>
          <td><%= request.updated_at.to_date %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
